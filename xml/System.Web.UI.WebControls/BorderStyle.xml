        <trans-unit id="101" translate="yes" xml:space="preserve" uid="T:System.Web.UI.WebControls.BorderStyle">
          <source>Specifies the border style of a control.</source>
          <target state="translated">Specifica lo stile del bordo di un controllo.</target>       </trans-unit>
        <trans-unit id="102" translate="yes" xml:space="preserve" extradata="MT" uid="T:System.Web.UI.WebControls.BorderStyle">
          <source>The <ph id="ph1">&lt;xref:System.Web.UI.WebControls.BorderStyle&gt;</ph> enumeration represents the different border style options for a control.</source>
          <target state="translated">Il <ph id="ph1">&lt;xref:System.Web.UI.WebControls.BorderStyle&gt;</ph> enumerazione rappresenta le opzioni di stile del bordo diversi per un controllo.</target>       </trans-unit>
        <trans-unit id="103" translate="yes" xml:space="preserve" extradata="MT" uid="T:System.Web.UI.WebControls.BorderStyle">
          <source>The width of the border is controlled by using the <ph id="ph1">&lt;xref:System.Web.UI.WebControls.WebControl.BorderWidth%2A&gt;</ph> property of the <ph id="ph2">&lt;xref:System.Web.UI.WebControls.WebControl&gt;</ph> class.</source>
          <target state="translated">La larghezza del bordo viene controllata tramite il <ph id="ph1">&lt;xref:System.Web.UI.WebControls.WebControl.BorderWidth%2A&gt;</ph> propriet√† la <ph id="ph2">&lt;xref:System.Web.UI.WebControls.WebControl&gt;</ph> classe.</target>       </trans-unit>
        <trans-unit id="104" translate="yes" xml:space="preserve" extradata="MT" uid="T:System.Web.UI.WebControls.BorderStyle">
          <source>When using the Double style, the border width you specify must be at least 3 pixels wide to accommodate both lines and the space in between.</source>
          <target state="translated">Quando si utilizza lo stile doppio, si specifica lo spessore del bordo deve essere almeno 3 pixel per regolare le linee e lo spazio al.</target>       </trans-unit>
        <trans-unit id="105" translate="yes" xml:space="preserve" uid="F:System.Web.UI.WebControls.BorderStyle.Dashed">
          <source>A dashed line border.</source>
          <target state="translated">Bordo a linea tratteggiata.</target>       </trans-unit>
        <trans-unit id="106" translate="yes" xml:space="preserve" uid="F:System.Web.UI.WebControls.BorderStyle.Dotted">
          <source>A dotted line border.</source>
          <target state="translated">Bordo a linea punteggiata.</target>       </trans-unit>
        <trans-unit id="107" translate="yes" xml:space="preserve" uid="F:System.Web.UI.WebControls.BorderStyle.Double">
          <source>A double solid line border.</source>
          <target state="translated">Bordo a linea continua doppia.</target>       </trans-unit>
        <trans-unit id="108" translate="yes" xml:space="preserve" uid="F:System.Web.UI.WebControls.BorderStyle.Groove">
          <source>A grooved border for a sunken border appearance.</source>
          <target state="translated">Bordo incassato per dare al bordo un aspetto incassato.</target>       </trans-unit>
        <trans-unit id="109" translate="yes" xml:space="preserve" uid="F:System.Web.UI.WebControls.BorderStyle.Inset">

        <trans-unit id="111" translate="yes" xml:space="preserve" uid="F:System.Web.UI.WebControls.BorderStyle.NotSet">
          <source>No set border style.</source>
          <target state="translated">Nessuno stile impostato per il bordo.</target>       </trans-unit>
        <trans-unit id="112" translate="yes" xml:space="preserve" uid="F:System.Web.UI.WebControls.BorderStyle.Outset">

      <group id="content" extype="content">
        <trans-unit id="101" translate="yes" xml:space="preserve" uid="T:System.DirectoryServices.SortDirection">
          <source>The <ph id="ph1">&lt;see cref="T:System.DirectoryServices.SortDirection" /&gt;</ph> enumeration specifies how to sort the results of an Active Directory Domain Services query.</source>
          <target state="translated">L'enumerazione <ph id="ph1">&lt;see cref="T:System.DirectoryServices.SortDirection" /&gt;</ph> specifica le modalità di ordinamento dei risultati di una query dei Servizi di dominio Active Directory.</target>       </trans-unit>
        <trans-unit id="102" translate="yes" xml:space="preserve" uid="F:System.DirectoryServices.SortDirection.Ascending">
          <source>Sort from smallest to largest.</source>
          <target state="translated">Ordina dal più piccolo al più grande.</target>       </trans-unit>
        <trans-unit id="103" translate="yes" xml:space="preserve" uid="F:System.DirectoryServices.SortDirection.Ascending">
          <source>For example, A to Z.</source>
          <target state="translated">Ad esempio, da A a Z.</target>       </trans-unit>
        <trans-unit id="104" translate="yes" xml:space="preserve" uid="F:System.DirectoryServices.SortDirection.Descending">
          <source>Sort from largest to smallest.</source>
          <target state="translated">Ordina dal più grande al più piccolo.</target>       </trans-unit>
        <trans-unit id="105" translate="yes" xml:space="preserve" uid="F:System.DirectoryServices.SortDirection.Descending">
          <source>For example, Z to A.</source>
          <target state="translated">Ad esempio, da Z a A.</target>       </trans-unit>

      <group id="content" extype="content">
        <trans-unit id="101" translate="yes" xml:space="preserve" uid="T:System.IO.MemoryMappedFiles.MemoryMappedFileAccess">
          <source>Specifies access capabilities and restrictions for a memory-mapped file or view.</source>
          <target state="translated">Specifica funzionalità e restrizioni di accesso per un file mappato alla memoria o una vista.</target>       </trans-unit>
        <trans-unit id="102" translate="yes" xml:space="preserve" uid="F:System.IO.MemoryMappedFiles.MemoryMappedFileAccess.CopyOnWrite">
          <source>Read and write access to the file, with the restriction that any write operations will not be seen by other processes.</source>
          <target state="translated">Accesso per leggere e scrivere in un file con la restrizione che le operazioni di scrittura non saranno viste dagli altri processi.</target>       </trans-unit>
        <trans-unit id="103" translate="yes" xml:space="preserve" uid="F:System.IO.MemoryMappedFiles.MemoryMappedFileAccess.Read">
          <source>Read-only access to the file.</source>
          <target state="translated">Accesso al file in sola lettura.</target>       </trans-unit>
        <trans-unit id="104" translate="yes" xml:space="preserve" uid="F:System.IO.MemoryMappedFiles.MemoryMappedFileAccess.ReadExecute">
          <source>Read access to the file that can store and run executable code.</source>
          <target state="translated">Accesso in lettura al file che può archiviare ed eseguire codice eseguibile.</target>       </trans-unit>
        <trans-unit id="105" translate="yes" xml:space="preserve" uid="F:System.IO.MemoryMappedFiles.MemoryMappedFileAccess.ReadWrite">
          <source>Read and write access to the file.</source>
          <target state="translated">Accesso in lettura/scrittura al file.</target>       </trans-unit>
        <trans-unit id="106" translate="yes" xml:space="preserve" uid="F:System.IO.MemoryMappedFiles.MemoryMappedFileAccess.ReadWriteExecute">
          <source>Read and write access to the file that can can store and run executable code.</source>
          <target state="translated">Accesso in lettura e scrittura al file che può archiviare ed eseguire codice eseguibile.</target>       </trans-unit>
        <trans-unit id="107" translate="yes" xml:space="preserve" uid="F:System.IO.MemoryMappedFiles.MemoryMappedFileAccess.Write">
          <source>Write-only access to file.</source>
          <target state="translated">Accesso al file in sola scrittura.</target>       </trans-unit>

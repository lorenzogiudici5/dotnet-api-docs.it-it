      <group id="content" extype="content">
        <trans-unit id="101" translate="yes" xml:space="preserve" uid="T:System.Data.Linq.IExecuteResult">
          <source>Provides access to the return value or results of executing a query.</source>
          <target state="translated">Fornisce l'accesso ai risultati o al valore restituito dell'esecuzione di una query.</target>       </trans-unit>
        <trans-unit id="102" translate="yes" xml:space="preserve" uid="M:System.Data.Linq.IExecuteResult.GetParameterValue(System.Int32)">
          <source>The index of the parameter to be retrieved.</source>
          <target state="translated">Indice del parametro da recuperare.</target>       </trans-unit>
        <trans-unit id="103" translate="yes" xml:space="preserve" uid="M:System.Data.Linq.IExecuteResult.GetParameterValue(System.Int32)">
          <source>Provides access to the <bpt id="p1">*</bpt>n<ept id="p1">*</ept>th output parameter.</source>
          <target state="translated">Specifica l'accesso al parametro di output con posizione <bpt id="p1">*</bpt>n<ept id="p1">*</ept>.</target>       </trans-unit>
        <trans-unit id="104" translate="yes" xml:space="preserve" uid="M:System.Data.Linq.IExecuteResult.GetParameterValue(System.Int32)">
          <source>An object that contains the value of the specified parameter.</source>
          <target state="translated">Oggetto contenente il valore del parametro specificato.</target>       </trans-unit>
        <trans-unit id="105" translate="yes" xml:space="preserve" extradata="MT" uid="M:System.Data.Linq.IExecuteResult.GetParameterValue(System.Int32)">
          <source>This method is typically used when the query is a mapped function that has output parameters.</source>
          <target state="translated">Questo metodo viene in genere utilizzato quando la query è una funzione mappata che contiene i parametri di output.</target>       </trans-unit>
        <trans-unit id="106" translate="yes" xml:space="preserve" uid="P:System.Data.Linq.IExecuteResult.ReturnValue">
          <source>Gets the return value or result of the executed query.</source>
          <target state="translated">Ottiene il valore restituito o il risultato della query eseguita.</target>       </trans-unit>
        <trans-unit id="107" translate="yes" xml:space="preserve" extradata="MT" uid="P:System.Data.Linq.IExecuteResult.ReturnValue">
          <source>The value or result of the executed query.</source>
          <target state="translated">Valore o risultato della query eseguita.</target>       </trans-unit>
        <trans-unit id="108" translate="yes" xml:space="preserve" extradata="MT" uid="P:System.Data.Linq.IExecuteResult.ReturnValue">

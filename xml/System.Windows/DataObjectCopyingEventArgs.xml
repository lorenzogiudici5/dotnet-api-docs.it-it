        <trans-unit id="101" translate="yes" xml:space="preserve" uid="T:System.Windows.DataObjectCopyingEventArgs">
          <source>Arguments for the <ph id="ph1">&lt;see cref="T:System.Windows.DataObject" /&gt;</ph>.<ph id="ph2">&lt;see cref="E:System.Windows.DataObject.Copying" /&gt;</ph></source>
          <target state="translated">Argomenti per l'evento <ph id="ph1">&lt;see cref="T:System.Windows.DataObject" /&gt;</ph>.<ph id="ph2">&lt;see cref="E:System.Windows.DataObject.Copying" /&gt;</ph></target>       </trans-unit>

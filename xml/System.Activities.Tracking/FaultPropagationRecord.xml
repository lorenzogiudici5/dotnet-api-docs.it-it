      <group id="content" extype="content">
        <trans-unit id="101" translate="yes" xml:space="preserve" uid="T:System.Activities.Tracking.FaultPropagationRecord">
          <source>Contains the data sent to a tracking participant by the run-time tracking infrastructure when a fault is propagated from a workflow activity.</source>
          <target state="translated">Contiene i dati inviati ad un'attività componente dall'infrastruttura di rilevamento di runtime quando viene propagato un errore da un'attività di un flusso di lavoro.</target>       </trans-unit>
        <trans-unit id="102" translate="yes" xml:space="preserve" uid="M:System.Activities.Tracking.FaultPropagationRecord.#ctor(System.Guid,System.Int64,System.Activities.Tracking.ActivityInfo,System.Activities.Tracking.ActivityInfo,System.Boolean,System.Exception)">
          <source>The instance ID of the activity that generated the event.</source>
          <target state="translated">ID dell'istanza dell'attività che ha generato l'evento.</target>       </trans-unit>
        <trans-unit id="103" translate="yes" xml:space="preserve" uid="M:System.Activities.Tracking.FaultPropagationRecord.#ctor(System.Guid,System.Int64,System.Activities.Tracking.ActivityInfo,System.Activities.Tracking.ActivityInfo,System.Boolean,System.Exception)">
          <source>A sequence that defines the order in which tracking events are generated.</source>
          <target state="translated">Una sequenza che definisce l'ordine in cui vengono generati gli eventi di rilevamento.</target>       </trans-unit>
        <trans-unit id="104" translate="yes" xml:space="preserve" uid="M:System.Activities.Tracking.FaultPropagationRecord.#ctor(System.Guid,System.Int64,System.Activities.Tracking.ActivityInfo,System.Activities.Tracking.ActivityInfo,System.Boolean,System.Exception)">
          <source>The activity that is the source of the fault.</source>
          <target state="translated">Attività origine dell'errore.</target>       </trans-unit>
        <trans-unit id="105" translate="yes" xml:space="preserve" uid="M:System.Activities.Tracking.FaultPropagationRecord.#ctor(System.Guid,System.Int64,System.Activities.Tracking.ActivityInfo,System.Activities.Tracking.ActivityInfo,System.Boolean,System.Exception)">
          <source>The activity that is the fault handler.</source>
          <target state="translated">Attività gestore dell'errore.</target>       </trans-unit>
        <trans-unit id="106" translate="yes" xml:space="preserve" uid="M:System.Activities.Tracking.FaultPropagationRecord.#ctor(System.Guid,System.Int64,System.Activities.Tracking.ActivityInfo,System.Activities.Tracking.ActivityInfo,System.Boolean,System.Exception)">
          <source><ph id="ph1">&lt;see langword="true" /&gt;</ph> if this handler is the first handler of the fault; <ph id="ph2">&lt;see langword="false" /&gt;</ph> if this handler is a subsequent handler of the fault.</source>
          <target state="translated"><ph id="ph1">&lt;see langword="true" /&gt;</ph> se questo è il primo gestore dell'errore; <ph id="ph2">&lt;see langword="false" /&gt;</ph> se questo gestore è un gestore successivo per l'errore.</target>       </trans-unit>
        <trans-unit id="107" translate="yes" xml:space="preserve" uid="M:System.Activities.Tracking.FaultPropagationRecord.#ctor(System.Guid,System.Int64,System.Activities.Tracking.ActivityInfo,System.Activities.Tracking.ActivityInfo,System.Boolean,System.Exception)">
          <source>The exception details.</source>
          <target state="translated">Informazioni dettagliate dell'eccezione.</target>       </trans-unit>
        <trans-unit id="108" translate="yes" xml:space="preserve" uid="M:System.Activities.Tracking.FaultPropagationRecord.#ctor(System.Guid,System.Int64,System.Activities.Tracking.ActivityInfo,System.Activities.Tracking.ActivityInfo,System.Boolean,System.Exception)">
          <source>Creates a new instance of the <ph id="ph1">&lt;see cref="T:System.Activities.Tracking.FaultPropagationRecord" /&gt;</ph> class.</source>
          <target state="translated">Crea una nuova istanza della classe <ph id="ph1">&lt;see cref="T:System.Activities.Tracking.FaultPropagationRecord" /&gt;</ph>.</target>       </trans-unit>
        <trans-unit id="109" translate="yes" xml:space="preserve" uid="P:System.Activities.Tracking.FaultPropagationRecord.Fault">
          <source>Get the exception data contained in the fault.</source>
          <target state="translated">Ottenere i dati dell'eccezione contenuti nell'errore.</target>       </trans-unit>
        <trans-unit id="110" translate="yes" xml:space="preserve" extradata="MT" uid="P:System.Activities.Tracking.FaultPropagationRecord.Fault">
          <source>The exception data.</source>
          <target state="translated">Dati dell'eccezione.</target>       </trans-unit>
        <trans-unit id="111" translate="yes" xml:space="preserve" uid="P:System.Activities.Tracking.FaultPropagationRecord.FaultHandler">
          <source>Gets the <ph id="ph1">&lt;see cref="T:System.Activities.Tracking.ActivityInfo" /&gt;</ph> of the fault handler.</source>
          <target state="translated">Ottiene <ph id="ph1">&lt;see cref="T:System.Activities.Tracking.ActivityInfo" /&gt;</ph> del gestore dell'errore.</target>       </trans-unit>
        <trans-unit id="112" translate="yes" xml:space="preserve" extradata="MT" uid="P:System.Activities.Tracking.FaultPropagationRecord.FaultHandler">
          <source>The activity information.</source>
          <target state="translated">Informazioni dell’attività.</target>       </trans-unit>
        <trans-unit id="113" translate="yes" xml:space="preserve" uid="P:System.Activities.Tracking.FaultPropagationRecord.FaultSource">
          <source>Gets the <ph id="ph1">&lt;see cref="T:System.Activities.Tracking.ActivityInfo" /&gt;</ph> of the activity that generated the fault.</source>
          <target state="translated">Ottiene la proprietà <ph id="ph1">&lt;see cref="T:System.Activities.Tracking.ActivityInfo" /&gt;</ph> dell'attività che ha generato l'errore.</target>       </trans-unit>
        <trans-unit id="114" translate="yes" xml:space="preserve" extradata="MT" uid="P:System.Activities.Tracking.FaultPropagationRecord.FaultSource">
          <source>The activity information.</source>
          <target state="translated">Informazioni dell’attività.</target>       </trans-unit>
        <trans-unit id="115" translate="yes" xml:space="preserve" uid="P:System.Activities.Tracking.FaultPropagationRecord.IsFaultSource">
          <source>A value that indicates whether this handler is the first handler for the fault.</source>
          <target state="translated">Un valore che indica se questo gestore è il primo gestore per l'errore.</target>       </trans-unit>
        <trans-unit id="116" translate="yes" xml:space="preserve" extradata="MT" uid="P:System.Activities.Tracking.FaultPropagationRecord.IsFaultSource">
          <source><ph id="ph1">&lt;see langword="true" /&gt;</ph> if this is the first handler for the exception.</source>
          <target state="translated"><ph id="ph1">&lt;see langword="true" /&gt;</ph> se questo è il primo gestore per l'eccezione.</target>       </trans-unit>
        <trans-unit id="117" translate="yes" xml:space="preserve" extradata="MT" uid="P:System.Activities.Tracking.FaultPropagationRecord.IsFaultSource">
          <source><ph id="ph1">&lt;see langword="false" /&gt;</ph> if this handler is a subsequent handler for the fault.</source>
          <target state="translated"><ph id="ph1">&lt;see langword="false" /&gt;</ph> se questo è un gestore successivo per l'eccezione.</target>       </trans-unit>
        <trans-unit id="118" translate="yes" xml:space="preserve" uid="M:System.Activities.Tracking.FaultPropagationRecord.ToString">
          <source>Returns a string that represents the current <ph id="ph1">&lt;see cref="T:System.Activities.Tracking.FaultPropagationRecord" /&gt;</ph> object.</source>
          <target state="translated">Restituisce una stringa che rappresenta l'oggetto <ph id="ph1">&lt;see cref="T:System.Activities.Tracking.FaultPropagationRecord" /&gt;</ph> corrente.</target>       </trans-unit>
        <trans-unit id="119" translate="yes" xml:space="preserve" uid="M:System.Activities.Tracking.FaultPropagationRecord.ToString">
          <source>The tracking data.</source>
          <target state="translated">Dati di rilevamento</target>       </trans-unit>

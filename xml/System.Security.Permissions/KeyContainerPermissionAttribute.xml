-   \#definire PROV_RSA_SIG 2  
  
-   \#definire PROV_DSS 3  
  
-   \#definire PROV_FORTEZZA 4  
  
-   \#definire PROV_MS_EXCHANGE 5  
  
-   \#definire PROV_SSL 6  
  
-   \#definire PROV_RSA_SCHANNEL 12  
  
-   \#define PROV_DSS_DH 13  
  
-   \#define PROV_EC_ECDSA_SIG 14  
  
-   \#define PROV_EC_ECNRA_SIG 15  
  
-   \#definire PROV_EC_ECDSA_FULL 16  
  
-   \#definire PROV_EC_ECNRA_FULL 17  
  
-   \#definire PROV_DH_SCHANNEL 18  
  
-   \#definire PROV_SPYRUS_LYNKS 20  
  
-   \#definire PROV_RNG 21  
  
-   \#define PROV_INTEL_SEC 22  
  
-   \#define PROV_REPLACE_OWF 23  
  
-   \#definire PROV_RSA_AES 24  
  
 ]]></format>

      <group id="content" extype="content">
        <trans-unit id="101" translate="yes" xml:space="preserve" uid="T:System.Security.Permissions.TypeDescriptorPermissionAttribute">
          <source>Determines the permission flags that apply to a <ph id="ph1">&lt;see cref="T:System.ComponentModel.TypeDescriptor" /&gt;</ph>.</source>
          <target state="translated">Determina i flag di autorizzazione che si applicano a un oggetto <ph id="ph1">&lt;see cref="T:System.ComponentModel.TypeDescriptor" /&gt;</ph>.</target>       </trans-unit>
        <trans-unit id="102" translate="yes" xml:space="preserve" uid="M:System.Security.Permissions.TypeDescriptorPermissionAttribute.#ctor(System.Security.Permissions.SecurityAction)">
          <source>One of the <ph id="ph1">&lt;see cref="T:System.Security.Permissions.SecurityAction" /&gt;</ph> values.</source>
          <target state="translated">Uno dei valori di <ph id="ph1">&lt;see cref="T:System.Security.Permissions.SecurityAction" /&gt;</ph>.</target>       </trans-unit>
        <trans-unit id="103" translate="yes" xml:space="preserve" uid="M:System.Security.Permissions.TypeDescriptorPermissionAttribute.#ctor(System.Security.Permissions.SecurityAction)">
          <source>Initializes a new instance of the <ph id="ph1">&lt;see cref="T:System.Security.Permissions.TypeDescriptorPermissionAttribute" /&gt;</ph> class with the specified <ph id="ph2">&lt;see cref="T:System.Security.Permissions.SecurityAction" /&gt;</ph>.</source>
          <target state="translated">Inizializza una nuova istanza della classe <ph id="ph1">&lt;see cref="T:System.Security.Permissions.TypeDescriptorPermissionAttribute" /&gt;</ph> con l'oggetto <ph id="ph2">&lt;see cref="T:System.Security.Permissions.SecurityAction" /&gt;</ph> specificato.</target>       </trans-unit>
        <trans-unit id="104" translate="yes" xml:space="preserve" uid="M:System.Security.Permissions.TypeDescriptorPermissionAttribute.CreatePermission">
          <source>When overridden in a derived class, creates a permission object that can then be serialized into binary form and persistently stored along with the <ph id="ph1">&lt;see cref="T:System.Security.Permissions.SecurityAction" /&gt;</ph> in an assembly's metadata.</source>
          <target state="translated">In caso di override in una classe derivata, crea un oggetto autorizzazioni che può quindi essere serializzato in formato binario e archiviato definitivamente con <ph id="ph1">&lt;see cref="T:System.Security.Permissions.SecurityAction" /&gt;</ph> nei metadati di un assembly.</target>       </trans-unit>
        <trans-unit id="105" translate="yes" xml:space="preserve" uid="M:System.Security.Permissions.TypeDescriptorPermissionAttribute.CreatePermission">
          <source>A serializable permission object.</source>
          <target state="translated">Oggetto autorizzazioni serializzabile.</target>       </trans-unit>
        <trans-unit id="106" translate="yes" xml:space="preserve" uid="P:System.Security.Permissions.TypeDescriptorPermissionAttribute.Flags">
          <source>Gets or sets the <ph id="ph1">&lt;see cref="T:System.Security.Permissions.TypeDescriptorPermissionFlags" /&gt;</ph> for the <ph id="ph2">&lt;see cref="T:System.ComponentModel.TypeDescriptor" /&gt;</ph>.</source>
          <target state="translated">Ottiene o imposta l'oggetto <ph id="ph1">&lt;see cref="T:System.Security.Permissions.TypeDescriptorPermissionFlags" /&gt;</ph> per <ph id="ph2">&lt;see cref="T:System.ComponentModel.TypeDescriptor" /&gt;</ph>.</target>       </trans-unit>
        <trans-unit id="107" translate="yes" xml:space="preserve" extradata="MT" uid="P:System.Security.Permissions.TypeDescriptorPermissionAttribute.Flags">
          <source>The <ph id="ph1">&lt;see cref="T:System.Security.Permissions.TypeDescriptorPermissionFlags" /&gt;</ph> for the <ph id="ph2">&lt;see cref="T:System.ComponentModel.TypeDescriptor" /&gt;</ph>.</source>
          <target state="translated">Oggetto <ph id="ph1">&lt;see cref="T:System.Security.Permissions.TypeDescriptorPermissionFlags" /&gt;</ph> per l'oggetto <ph id="ph2">&lt;see cref="T:System.ComponentModel.TypeDescriptor" /&gt;</ph>.</target>       </trans-unit>
        <trans-unit id="108" translate="yes" xml:space="preserve" uid="P:System.Security.Permissions.TypeDescriptorPermissionAttribute.RestrictedRegistrationAccess">
          <source>Gets or sets a value that indicates whether the type descriptor can be accessed from partial trust.</source>
          <target state="translated">Ottiene o imposta un valore che indica se l'accesso al descrittore di tipo può essere eseguito da un contesto di attendibilità parziale.</target>       </trans-unit>
        <trans-unit id="109" translate="yes" xml:space="preserve" extradata="MT" uid="P:System.Security.Permissions.TypeDescriptorPermissionAttribute.RestrictedRegistrationAccess">
          <source><ph id="ph1">&lt;see langword="true" /&gt;</ph> if the type descriptor can be accessed from partial trust; otherwise, <ph id="ph2">&lt;see langword="false" /&gt;</ph>.</source>
          <target state="translated"><ph id="ph1">&lt;see langword="true" /&gt;</ph> se l'accesso al descrittore di tipo può essere eseguito da un contesto di attendibilità parziale. In caso contrario, <ph id="ph2">&lt;see langword="false" /&gt;</ph>.</target>       </trans-unit>

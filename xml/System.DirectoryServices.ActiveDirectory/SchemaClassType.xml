        <trans-unit id="101" translate="yes" xml:space="preserve" uid="T:System.DirectoryServices.ActiveDirectory.SchemaClassType">
          <source>Specifies the class type of an <ph id="ph1">&lt;see cref="T:System.DirectoryServices.ActiveDirectory.ActiveDirectorySchemaClass" /&gt;</ph> object.</source>
          <target state="translated">Specifica il tipo di classe di un oggetto <ph id="ph1">&lt;see cref="T:System.DirectoryServices.ActiveDirectory.ActiveDirectorySchemaClass" /&gt;</ph>.</target>       </trans-unit>
        <trans-unit id="102" translate="yes" xml:space="preserve" uid="F:System.DirectoryServices.ActiveDirectory.SchemaClassType.Abstract">
          <source>The class is an abstract class.</source>
          <target state="translated">Indica una classe astratta.</target>       </trans-unit>
        <trans-unit id="103" translate="yes" xml:space="preserve" uid="F:System.DirectoryServices.ActiveDirectory.SchemaClassType.Auxiliary">
          <source>The class is an auxiliary class.</source>
          <target state="translated">Indica una classe ausiliaria.</target>       </trans-unit>
        <trans-unit id="104" translate="yes" xml:space="preserve" uid="F:System.DirectoryServices.ActiveDirectory.SchemaClassType.Structural">

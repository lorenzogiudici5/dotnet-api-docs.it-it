      <group id="content" extype="content">
        <trans-unit id="101" translate="yes" xml:space="preserve" uid="T:System.DirectoryServices.Protocols.SearchScope">
          <source>The <ph id="ph1">&lt;see cref="T:System.DirectoryServices.Protocols.SearchScope" /&gt;</ph> enumeration specifies the scope of a search.</source>
          <target state="translated">L'enumerazione <ph id="ph1">&lt;see cref="T:System.DirectoryServices.Protocols.SearchScope" /&gt;</ph> specifica l'ambito di una ricerca.</target>       </trans-unit>
        <trans-unit id="102" translate="yes" xml:space="preserve" uid="F:System.DirectoryServices.Protocols.SearchScope.Base">
          <source>Search only the specified base object.</source>
          <target state="translated">La ricerca viene eseguita solo nell'oggetto base specificato.</target>       </trans-unit>
        <trans-unit id="103" translate="yes" xml:space="preserve" uid="F:System.DirectoryServices.Protocols.SearchScope.Base">
          <source>The value is equal to 0.</source>
          <target state="translated">Il valore è pari a 0.</target>       </trans-unit>
        <trans-unit id="104" translate="yes" xml:space="preserve" uid="F:System.DirectoryServices.Protocols.SearchScope.OneLevel">
          <source>Search the child objects of the base object, but not the base object itself.</source>
          <target state="translated">La ricerca viene eseguita negli oggetti figlio dell'oggetto base, escludendo l'oggetto base stesso.</target>       </trans-unit>
        <trans-unit id="105" translate="yes" xml:space="preserve" uid="F:System.DirectoryServices.Protocols.SearchScope.OneLevel">
          <source>The value is equal to 1.</source>
          <target state="translated">Il valore è pari a 1.</target>       </trans-unit>
        <trans-unit id="106" translate="yes" xml:space="preserve" uid="F:System.DirectoryServices.Protocols.SearchScope.Subtree">
          <source>Search the base object and all child objects.</source>
          <target state="translated">La ricerca viene eseguita nell'oggetto base e in tutti gli oggetti figlio.</target>       </trans-unit>
        <trans-unit id="107" translate="yes" xml:space="preserve" uid="F:System.DirectoryServices.Protocols.SearchScope.Subtree">
          <source>The value is equal to 2.</source>
          <target state="translated">Il valore è pari a 2.</target>       </trans-unit>

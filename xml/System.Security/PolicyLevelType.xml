      <group id="content" extype="content">
        <trans-unit id="101" translate="yes" xml:space="preserve" uid="T:System.Security.PolicyLevelType">
          <source>Specifies the type of a managed code policy level.</source>
          <target state="translated">Specifica il tipo di un livello di criteri di codice gestito.</target>       </trans-unit>
        <trans-unit id="102" translate="yes" xml:space="preserve" extradata="MT" uid="T:System.Security.PolicyLevelType">
          <source>The highest level of security policy is enterprise-wide.</source>
          <target state="translated">Il massimo livello di criteri di sicurezza è a livello aziendale.</target>       </trans-unit>
        <trans-unit id="103" translate="yes" xml:space="preserve" extradata="MT" uid="T:System.Security.PolicyLevelType">
          <source>Successive lower levels of hierarchy represent further policy restrictions, but can never grant more permissions than allowed by higher levels.</source>
          <target state="translated">Successivi livelli inferiori della gerarchia rappresentano ulteriori restrizioni dei criteri, ma mai possano concedere autorizzazioni a quella consentita dalla livelli più elevati.</target>       </trans-unit>
        <trans-unit id="104" translate="yes" xml:space="preserve" extradata="MT" uid="T:System.Security.PolicyLevelType">
          <source>The policy levels in order are the following.</source>
          <target state="translated">I livelli di criteri nell'ordine sono i seguenti.</target>       </trans-unit>
        <trans-unit id="105" translate="yes" xml:space="preserve" extradata="MT" uid="T:System.Security.PolicyLevelType">
          <source>Enterprise</source>
          <target state="translated">Enterprise</target>       </trans-unit>
        <trans-unit id="106" translate="yes" xml:space="preserve" extradata="MT" uid="T:System.Security.PolicyLevelType">
          <source>Machine</source>
          <target state="translated">Computer</target>       </trans-unit>
        <trans-unit id="107" translate="yes" xml:space="preserve" extradata="MT" uid="T:System.Security.PolicyLevelType">
          <source>User</source>
          <target state="translated">Utente</target>       </trans-unit>
        <trans-unit id="108" translate="yes" xml:space="preserve" extradata="MT" uid="T:System.Security.PolicyLevelType">
          <source>Application domain</source>
          <target state="translated">Dominio dell'applicazione</target>       </trans-unit>

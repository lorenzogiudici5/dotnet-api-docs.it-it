        <trans-unit id="101" translate="yes" xml:space="preserve" uid="T:System.Data.Services.IExpandedResult">
          <source>Declares the members required to support enumerators for results and associated segments on an WCF Data Services <bpt id="p1">&lt;c&gt;</bpt>$expand<ept id="p1">&lt;/c&gt;</ept> query option.</source>
          <target state="translated">Dichiara i membri necessari per supportare gli enumeratori dei risultati e dei segmenti associati in un'opzione query <bpt id="p1">&lt;c&gt;</bpt>$expand<ept id="p1">&lt;/c&gt;</ept> di WCF Data Services.</target>       </trans-unit>
        <trans-unit id="102" translate="yes" xml:space="preserve" extradata="MT" uid="T:System.Data.Services.IExpandedResult">
          <source>Expand providers can generate enumerations that implement this interface to reshape expanded properties.</source>
          <target state="translated">I provider di espansione possono generare enumerazioni che implementano questa interfaccia per rielaborare le proprietà espanse.</target>       </trans-unit>
        <trans-unit id="103" translate="yes" xml:space="preserve" uid="P:System.Data.Services.IExpandedResult.ExpandedElement">
          <source>Gets the element with expanded properties.</source>
          <target state="translated">Ottiene l'elemento con le proprietà espanse.</target>       </trans-unit>
        <trans-unit id="104" translate="yes" xml:space="preserve" extradata="MT" uid="P:System.Data.Services.IExpandedResult.ExpandedElement">
          <source>The object in a property expanded by <ph id="ph1">&lt;see cref="T:System.Data.Services.IExpandedResult" /&gt;</ph>.</source>
          <target state="translated">Oggetto in una proprietà espansa da <ph id="ph1">&lt;see cref="T:System.Data.Services.IExpandedResult" /&gt;</ph>.</target>       </trans-unit>
        <trans-unit id="105" translate="yes" xml:space="preserve" uid="M:System.Data.Services.IExpandedResult.GetExpandedPropertyValue(System.String)">
          <source>The name of the property for which to get enumerable results.</source>
          <target state="translated">Nome della proprietà per il quale ottenere risultati enumerabili.</target>       </trans-unit>
        <trans-unit id="106" translate="yes" xml:space="preserve" uid="M:System.Data.Services.IExpandedResult.GetExpandedPropertyValue(System.String)">
          <source>Gets the value for a named property of the result.</source>
          <target state="translated">Ottiene il valore per una proprietà denominata del risultato.</target>       </trans-unit>

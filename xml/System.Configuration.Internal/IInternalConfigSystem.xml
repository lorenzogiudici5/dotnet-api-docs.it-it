        <trans-unit id="101" translate="yes" xml:space="preserve" uid="T:System.Configuration.Internal.IInternalConfigSystem">
          <source>Defines an interface used by the .NET Framework to initialize application configuration properties.</source>
          <target state="translated">Definisce un'interfaccia utilizzata da .NET Framework per inizializzare le proprietà di configurazione dell'applicazione.</target>       </trans-unit>
        <trans-unit id="102" translate="yes" xml:space="preserve" uid="M:System.Configuration.Internal.IInternalConfigSystem.GetSection(System.String)">
          <source>The configuration key value.</source>
          <target state="translated">Valore della chiave di configurazione.</target>       </trans-unit>
        <trans-unit id="103" translate="yes" xml:space="preserve" uid="M:System.Configuration.Internal.IInternalConfigSystem.GetSection(System.String)">
          <source>Returns the configuration object based on the specified key.</source>
          <target state="translated">Restituisce l'oggetto configurazione in base alla chiave specificata.</target>       </trans-unit>
        <trans-unit id="104" translate="yes" xml:space="preserve" uid="M:System.Configuration.Internal.IInternalConfigSystem.GetSection(System.String)">
          <source>A configuration object.</source>
          <target state="translated">Oggetto di configurazione.</target>       </trans-unit>
        <trans-unit id="105" translate="yes" xml:space="preserve" extradata="MT" uid="M:System.Configuration.Internal.IInternalConfigSystem.GetSection(System.String)">

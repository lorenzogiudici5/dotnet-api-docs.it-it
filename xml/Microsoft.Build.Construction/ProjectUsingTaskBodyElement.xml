    <body>
      <group id="content" extype="content">
        <trans-unit id="101" translate="yes" xml:space="preserve" uid="T:Microsoft.Build.Construction.ProjectUsingTaskBodyElement">
          <source>Represents the body of an inline task.</source>
          <target state="translated">Rappresenta il corpo di un'attività inline.</target>       </trans-unit>
        <trans-unit id="102" translate="yes" xml:space="preserve" uid="P:Microsoft.Build.Construction.ProjectUsingTaskBodyElement.Condition">
          <source>Gets null because the Condition attribute is nonexistent for this element and a nonexistent condition is implicitly true.</source>
          <target state="translated">Ottiene il valore null perché l'attributo Condition è inesistente per questo elemento e una condizione inesistente è implicitamente true.</target>       </trans-unit>
        <trans-unit id="103" translate="yes" xml:space="preserve" extradata="MT" uid="P:Microsoft.Build.Construction.ProjectUsingTaskBodyElement.Condition">
          <source>Returns a null.</source>
          <target state="translated">Restituisce null.</target>       </trans-unit>
        <trans-unit id="104" translate="yes" xml:space="preserve" uid="P:Microsoft.Build.Construction.ProjectUsingTaskBodyElement.ConditionLocation">
          <source>Do not use.</source>
          <target state="translated">Non usare.</target>       </trans-unit>
        <trans-unit id="105" translate="yes" xml:space="preserve" uid="P:Microsoft.Build.Construction.ProjectUsingTaskBodyElement.ConditionLocation">
          <source>The Condition attribute is nonexistent for <ph id="ph1">&lt;see cref="T:Microsoft.Build.Construction.ProjectUsingTaskBodyElement" /&gt;</ph>.</source>
          <target state="translated">L'attributo Condition è inesistente per <ph id="ph1">&lt;see cref="T:Microsoft.Build.Construction.ProjectUsingTaskBodyElement" /&gt;</ph>.</target>       </trans-unit>
        <trans-unit id="106" translate="yes" xml:space="preserve" uid="P:Microsoft.Build.Construction.ProjectUsingTaskBodyElement.ConditionLocation">
          <source>Get throws an internal error.</source>
          <target state="translated">Ottiene le generazioni di un errore interno.</target>       </trans-unit>
        <trans-unit id="107" translate="yes" xml:space="preserve" extradata="MT" uid="P:Microsoft.Build.Construction.ProjectUsingTaskBodyElement.ConditionLocation">
          <source>Throws an internal error and returns null.</source>
          <target state="translated">Genera un errore interno e un restituisce un valore null.</target>       </trans-unit>
        <trans-unit id="108" translate="yes" xml:space="preserve" uid="P:Microsoft.Build.Construction.ProjectUsingTaskBodyElement.Evaluate">
          <source>Gets the value of the Evaluate attribute.</source>
          <target state="translated">Ottiene il valore dell'attributo Evaluate.</target>       </trans-unit>
        <trans-unit id="109" translate="yes" xml:space="preserve" extradata="MT" uid="P:Microsoft.Build.Construction.ProjectUsingTaskBodyElement.Evaluate">
          <source>Returns the value of the Evaluate attribute, which is either "true" or "false".</source>
          <target state="translated">Restituisce il valore dell'attributo Evaluate, che può essere "true" o "false".</target>       </trans-unit>
        <trans-unit id="110" translate="yes" xml:space="preserve" extradata="MT" uid="P:Microsoft.Build.Construction.ProjectUsingTaskBodyElement.Evaluate">
          <source>Returns "true" if the attribute is not present.</source>
          <target state="translated">Restituisce "true" se l'attributo non è presente.</target>       </trans-unit>
        <trans-unit id="111" translate="yes" xml:space="preserve" extradata="MT" uid="P:Microsoft.Build.Construction.ProjectUsingTaskBodyElement.Evaluate">
          <source>If "true", expands the item and property values in the inline task body.</source>
          <target state="translated">Se è "true", espande i valori di proprietà e l'elemento nel corpo dell'attività inline.</target>       </trans-unit>
        <trans-unit id="112" translate="yes" xml:space="preserve" uid="P:Microsoft.Build.Construction.ProjectUsingTaskBodyElement.EvaluateLocation">
          <source>Location of the Evaluate attribute on this element, if any.</source>
          <target state="translated">Posizione dell'attributo Evaluate sull'elemento, se disponibile.</target>       </trans-unit>
        <trans-unit id="113" translate="yes" xml:space="preserve" extradata="MT" uid="P:Microsoft.Build.Construction.ProjectUsingTaskBodyElement.EvaluateLocation">
          <source>Returns Evaluate attribute location.</source>
          <target state="translated">Posizione la posizione dell'attributo Evaluate.</target>       </trans-unit>
        <trans-unit id="114" translate="yes" xml:space="preserve" extradata="MT" uid="P:Microsoft.Build.Construction.ProjectUsingTaskBodyElement.EvaluateLocation">
          <source>If there is no such attribute, returns the location of the element, instead of the location of the default attribute.</source>
          <target state="translated">In assenza di tale attributo, restituisce la posizione dell'elemento, anziché la posizione dell'attributo predefinito.</target>       </trans-unit>
        <trans-unit id="115" translate="yes" xml:space="preserve" uid="P:Microsoft.Build.Construction.ProjectUsingTaskBodyElement.TaskBody">
          <source>Gets or sets the unevaluated value of the contents of the inline task.</source>
          <target state="translated">Ottiene o imposta il valore non valutato del contenuto dell'attività inline.</target>       </trans-unit>
        <trans-unit id="116" translate="yes" xml:space="preserve" extradata="MT" uid="P:Microsoft.Build.Construction.ProjectUsingTaskBodyElement.TaskBody">
          <source>Returns the unevaluated inner XML content of the inline task.</source>
          <target state="translated">Restituisce il contenuto XML interno non valutato dell'attività inline.</target>       </trans-unit>
        <trans-unit id="117" translate="yes" xml:space="preserve" extradata="MT" uid="P:Microsoft.Build.Construction.ProjectUsingTaskBodyElement.TaskBody">
          <source>Returns an empty string if no inline task body is present.</source>
          <target state="translated">Restituisce una stringa vuota se non è presente alcun corpo dell'attività inline.</target>       </trans-unit>
      </group>

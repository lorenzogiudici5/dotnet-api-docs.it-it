        <trans-unit id="102" translate="yes" xml:space="preserve" uid="T:System.Net.OpenWriteCompletedEventHandler">
          <source>A <ph id="ph1">&lt;see cref="T:System.Net.OpenWriteCompletedEventArgs" /&gt;</ph> containing event data.</source>
          <target state="translated">Oggetto <ph id="ph1">&lt;see cref="T:System.Net.OpenWriteCompletedEventArgs" /&gt;</ph> contenente i dati dell'evento.</target>       </trans-unit>
        <trans-unit id="103" translate="yes" xml:space="preserve" uid="T:System.Net.OpenWriteCompletedEventHandler">
          <source>Represents the method that will handle the <ph id="ph1">&lt;see cref="E:System.Net.WebClient.OpenWriteCompleted" /&gt;</ph> event of a <ph id="ph2">&lt;see cref="T:System.Net.WebClient" /&gt;</ph>.</source>

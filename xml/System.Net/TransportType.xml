        <trans-unit id="101" translate="yes" xml:space="preserve" uid="T:System.Net.TransportType">
          <source>Defines transport types for the <ph id="ph1">&lt;see cref="T:System.Net.SocketPermission" /&gt;</ph> and <ph id="ph2">&lt;see cref="T:System.Net.Sockets.Socket" /&gt;</ph> classes.</source>
          <target state="translated">Definisce i tipi di trasporto per le classi <ph id="ph1">&lt;see cref="T:System.Net.SocketPermission" /&gt;</ph> e <ph id="ph2">&lt;see cref="T:System.Net.Sockets.Socket" /&gt;</ph>.</target>       </trans-unit>
        <trans-unit id="102" translate="yes" xml:space="preserve" extradata="MT" uid="T:System.Net.TransportType">
          <source>The <ph id="ph1">&lt;xref:System.Net.TransportType&gt;</ph> enumeration defines transport types for the <ph id="ph2">&lt;xref:System.Net.SocketPermission&gt;</ph> and <ph id="ph3">&lt;xref:System.Net.Sockets.Socket&gt;</ph> classes.</source>
          <target state="translated">Il <ph id="ph1">&lt;xref:System.Net.TransportType&gt;</ph> enumerazione definisce i tipi di trasporto per il <ph id="ph2">&lt;xref:System.Net.SocketPermission&gt;</ph> e <ph id="ph3">&lt;xref:System.Net.Sockets.Socket&gt;</ph> classi.</target>       </trans-unit>
        <trans-unit id="103" translate="yes" xml:space="preserve" uid="F:System.Net.TransportType.All">
          <source>All transport types.</source>
          <target state="translated">Tutti i tipi di trasporto.</target>       </trans-unit>
        <trans-unit id="104" translate="yes" xml:space="preserve" uid="F:System.Net.TransportType.Connectionless">
          <source>The transport type is connectionless, such as UDP.</source>
          <target state="translated">Il tipo di trasporto è senza connessione, come UDP.</target>       </trans-unit>
        <trans-unit id="105" translate="yes" xml:space="preserve" uid="F:System.Net.TransportType.Connectionless">
          <source>Specifying this value has the same effect as specifying <ph id="ph1">&lt;see cref="F:System.Net.TransportType.Udp" /&gt;</ph>.</source>
          <target state="translated">L'impostazione di questo valore ha lo stesso effetto dell'impostazione di <ph id="ph1">&lt;see cref="F:System.Net.TransportType.Udp" /&gt;</ph>.</target>       </trans-unit>
        <trans-unit id="106" translate="yes" xml:space="preserve" uid="F:System.Net.TransportType.ConnectionOriented">
          <source>The transport is connection oriented, such as TCP.</source>
          <target state="translated">Il tipo di trasporto è orientato alla connessione, come TCP.</target>       </trans-unit>
        <trans-unit id="107" translate="yes" xml:space="preserve" uid="F:System.Net.TransportType.ConnectionOriented">
          <source>Specifying this value has the same effect as specifying <ph id="ph1">&lt;see cref="F:System.Net.TransportType.Tcp" /&gt;</ph>.</source>
          <target state="translated">L'impostazione di questo valore ha lo stesso effetto dell'impostazione di <ph id="ph1">&lt;see cref="F:System.Net.TransportType.Tcp" /&gt;</ph>.</target>       </trans-unit>

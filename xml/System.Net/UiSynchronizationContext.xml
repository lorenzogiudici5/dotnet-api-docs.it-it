        <trans-unit id="101" translate="yes" xml:space="preserve" uid="T:System.Net.UiSynchronizationContext">
          <source>Provides the synchronization context for the managed UI used in synchronization models.</source>
          <target state="translated">Fornisce il contesto di sincronizzazione per l'interfaccia utente gestita utilizzata nei modelli di sincronizzazione.</target>       </trans-unit>
        <trans-unit id="102" translate="yes" xml:space="preserve" uid="P:System.Net.UiSynchronizationContext.Current">
          <source>Gets the synchronization context for the current thread.</source>
          <target state="translated">Ottiene il contesto di sincronizzazione per il thread corrente.</target>       </trans-unit>
        <trans-unit id="103" translate="yes" xml:space="preserve" extradata="MT" uid="P:System.Net.UiSynchronizationContext.Current">
          <source>Returns <ph id="ph1">&lt;see cref="T:System.Threading.SynchronizationContext" /&gt;</ph>.</source>
          <target state="translated">Restituisce <ph id="ph1">&lt;see cref="T:System.Threading.SynchronizationContext" /&gt;</ph>.</target>       </trans-unit>
        <trans-unit id="104" translate="yes" xml:space="preserve" extradata="MT" uid="P:System.Net.UiSynchronizationContext.Current">
          <source>The synchronization context for the current thread.</source>
          <target state="translated">Contesto di sincronizzazione per il thread corrente.</target>       </trans-unit>
        <trans-unit id="105" translate="yes" xml:space="preserve" uid="P:System.Net.UiSynchronizationContext.ManagedUiThreadId">
          <source>Gets a unique identifier for the current managed thread.</source>
          <target state="translated">Ottiene un identificatore univoco per il thread gestito corrente.</target>       </trans-unit>
        <trans-unit id="106" translate="yes" xml:space="preserve" extradata="MT" uid="P:System.Net.UiSynchronizationContext.ManagedUiThreadId">

      <group id="content" extype="content">
        <trans-unit id="101" translate="yes" xml:space="preserve" uid="T:System.Web.ModelBinding.DataAnnotationsModelMetadata">
          <source>Provides a container for common metadata, for the <ph id="ph1">&lt;see cref="T:System.Web.ModelBinding.DataAnnotationsModelMetadataProvider" /&gt;</ph> class, and for the <ph id="ph2">&lt;see cref="T:System.Web.ModelBinding.DataAnnotationsModelValidator" /&gt;</ph> class for a data model.</source>
          <target state="translated">Fornisce un contenitore per metadati comuni, per la classe <ph id="ph1">&lt;see cref="T:System.Web.ModelBinding.DataAnnotationsModelMetadataProvider" /&gt;</ph> e per la classe <ph id="ph2">&lt;see cref="T:System.Web.ModelBinding.DataAnnotationsModelValidator" /&gt;</ph> per un modello dati.</target>       </trans-unit>
        <trans-unit id="102" translate="yes" xml:space="preserve" uid="M:System.Web.ModelBinding.DataAnnotationsModelMetadata.#ctor(System.Web.ModelBinding.DataAnnotationsModelMetadataProvider,System.Type,System.Func{System.Object},System.Type,System.String,System.ComponentModel.DataAnnotations.DisplayColumnAttribute)">
          <source>The provider object.</source>
          <target state="translated">Oggetto provider.</target>       </trans-unit>
        <trans-unit id="103" translate="yes" xml:space="preserve" uid="M:System.Web.ModelBinding.DataAnnotationsModelMetadata.#ctor(System.Web.ModelBinding.DataAnnotationsModelMetadataProvider,System.Type,System.Func{System.Object},System.Type,System.String,System.ComponentModel.DataAnnotations.DisplayColumnAttribute)">
          <source>The type of the container, or <ph id="ph1">&lt;see langword="null" /&gt;</ph> if there is no container.</source>
          <target state="translated">Tipo del contenitore, o <ph id="ph1">&lt;see langword="null" /&gt;</ph> se non c'è un contenitore.</target>       </trans-unit>
        <trans-unit id="104" translate="yes" xml:space="preserve" uid="M:System.Web.ModelBinding.DataAnnotationsModelMetadata.#ctor(System.Web.ModelBinding.DataAnnotationsModelMetadataProvider,System.Type,System.Func{System.Object},System.Type,System.String,System.ComponentModel.DataAnnotations.DisplayColumnAttribute)">
          <source>The model accessor.</source>
          <target state="translated">Funzione di accesso del modello.</target>       </trans-unit>
        <trans-unit id="105" translate="yes" xml:space="preserve" uid="M:System.Web.ModelBinding.DataAnnotationsModelMetadata.#ctor(System.Web.ModelBinding.DataAnnotationsModelMetadataProvider,System.Type,System.Func{System.Object},System.Type,System.String,System.ComponentModel.DataAnnotations.DisplayColumnAttribute)">
          <source>The type of the model.</source>
          <target state="translated">Tipo del modello.</target>       </trans-unit>
        <trans-unit id="106" translate="yes" xml:space="preserve" uid="M:System.Web.ModelBinding.DataAnnotationsModelMetadata.#ctor(System.Web.ModelBinding.DataAnnotationsModelMetadataProvider,System.Type,System.Func{System.Object},System.Type,System.String,System.ComponentModel.DataAnnotations.DisplayColumnAttribute)">
          <source>The name of the property, or <ph id="ph1">&lt;see langword="null" /&gt;</ph> if the model is not a property.</source>
          <target state="translated">Nome della proprietà o <ph id="ph1">&lt;see langword="null" /&gt;</ph> se il modello non è una proprietà.</target>       </trans-unit>
        <trans-unit id="107" translate="yes" xml:space="preserve" uid="M:System.Web.ModelBinding.DataAnnotationsModelMetadata.#ctor(System.Web.ModelBinding.DataAnnotationsModelMetadataProvider,System.Type,System.Func{System.Object},System.Type,System.String,System.ComponentModel.DataAnnotations.DisplayColumnAttribute)">
          <source>The display column attribute.</source>
          <target state="translated">Attributo della colonna di visualizzazione.</target>       </trans-unit>
        <trans-unit id="108" translate="yes" xml:space="preserve" uid="M:System.Web.ModelBinding.DataAnnotationsModelMetadata.#ctor(System.Web.ModelBinding.DataAnnotationsModelMetadataProvider,System.Type,System.Func{System.Object},System.Type,System.String,System.ComponentModel.DataAnnotations.DisplayColumnAttribute)">

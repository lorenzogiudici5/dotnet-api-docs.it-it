      <MemberSignature Language="ILAsm" Value=".property instance bool GenerateTemporaryTargetAssemblyDebuggingInformation" />
      <MemberSignature Language="DocId" Value="P:Microsoft.Build.Tasks.Windows.GenerateTemporaryTargetAssembly.GenerateTemporaryTargetAssemblyDebuggingInformation" />

      <group id="content" extype="content">
        <trans-unit id="101" translate="yes" xml:space="preserve" uid="T:System.Web.UI.DataVisualization.Charting.DateRangeType">
          <source>Specifies a date/time interval that determines the ranges that will be filtered when you invoke one of the <bpt id="p1">&lt;c&gt;</bpt>Filter<ept id="p1">&lt;/c&gt;</ept> method definitionsfound in the <ph id="ph1">&lt;see cref="T:System.Web.UI.DataVisualization.Charting.DataManipulator" /&gt;</ph> class.</source>
          <target state="translated">Specifica un intervallo di data/ora che determina gli intervalli che verranno filtrati quando si richiama una delle definizioni del metodo <bpt id="p1">&lt;c&gt;</bpt>Filter<ept id="p1">&lt;/c&gt;</ept> disponibili nella classe <ph id="ph1">&lt;see cref="T:System.Web.UI.DataVisualization.Charting.DataManipulator" /&gt;</ph>.</target>       </trans-unit>
        <trans-unit id="102" translate="yes" xml:space="preserve" extradata="MT" uid="T:System.Web.UI.DataVisualization.Charting.DateRangeType">
          <source>The <ph id="ph1">&lt;xref:System.Web.UI.DataVisualization.Charting.DateRangeType&gt;</ph> enumeration is passed in the <ph id="ph2">`DateRange`</ph> parameter of some <ph id="ph3">`Filter`</ph> methods, which combined with the <ph id="ph4">`rangeElements`</ph> parameter determines the date/time ranges that are filtered.</source>
          <target state="translated">Il <ph id="ph1">&lt;xref:System.Web.UI.DataVisualization.Charting.DateRangeType&gt;</ph> enumerazione viene passato il <ph id="ph2">`DateRange`</ph> parametro di alcuni <ph id="ph3">`Filter`</ph> metodi, combinato con il <ph id="ph4">`rangeElements`</ph> parametro determina gli intervalli di data/ora vengono filtrati.</target>       </trans-unit>
        <trans-unit id="103" translate="yes" xml:space="preserve" uid="F:System.Web.UI.DataVisualization.Charting.DateRangeType.DayOfMonth">
          <source>Date/time interval is days of the month.</source>
          <target state="translated">L'intervallo di data/ora è espresso in giorni del mese.</target>       </trans-unit>
        <trans-unit id="104" translate="yes" xml:space="preserve" uid="F:System.Web.UI.DataVisualization.Charting.DateRangeType.DayOfWeek">
          <source>Date/time interval is days of the week.</source>
          <target state="translated">L'intervallo di data/ora è espresso in giorni della settimana.</target>       </trans-unit>
        <trans-unit id="105" translate="yes" xml:space="preserve" uid="F:System.Web.UI.DataVisualization.Charting.DateRangeType.Hour">
          <source>Date/time interval is hours.</source>
          <target state="translated">L'intervallo di data/ora è espresso in ore.</target>       </trans-unit>
        <trans-unit id="106" translate="yes" xml:space="preserve" uid="F:System.Web.UI.DataVisualization.Charting.DateRangeType.Minute">
          <source>Date/time interval is minutes.</source>
          <target state="translated">L'intervallo di data/ora è espresso in minuti.</target>       </trans-unit>
        <trans-unit id="107" translate="yes" xml:space="preserve" uid="F:System.Web.UI.DataVisualization.Charting.DateRangeType.Month">
          <source>Date/time interval is months.</source>
          <target state="translated">L'intervallo di data/ora è espresso in mesi.</target>       </trans-unit>
        <trans-unit id="108" translate="yes" xml:space="preserve" uid="F:System.Web.UI.DataVisualization.Charting.DateRangeType.Year">
          <source>Date/time interval is years.</source>
          <target state="translated">L'intervallo di data/ora è espresso in anni.</target>       </trans-unit>

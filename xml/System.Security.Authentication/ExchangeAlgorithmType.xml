        <trans-unit id="101" translate="yes" xml:space="preserve" uid="T:System.Security.Authentication.ExchangeAlgorithmType">
          <source>Specifies the algorithm used to create keys shared by the client and server.</source>
          <target state="translated">Specifica l'algoritmo usato per creare chiavi condivise dal client e dal server.</target>       </trans-unit>
        <trans-unit id="102" translate="yes" xml:space="preserve" extradata="MT" uid="T:System.Security.Authentication.ExchangeAlgorithmType">
          <source>This enumeration specifies valid values for the <ph id="ph1">&lt;xref:System.Net.Security.SslStream.KeyExchangeAlgorithm%2A?displayProperty=nameWithType&gt;</ph> property.</source>
          <target state="translated">Questa enumerazione specifica i valori validi per il <ph id="ph1">&lt;xref:System.Net.Security.SslStream.KeyExchangeAlgorithm%2A?displayProperty=nameWithType&gt;</ph> proprietà.</target>       </trans-unit>
        <trans-unit id="103" translate="yes" xml:space="preserve" extradata="MT" uid="T:System.Security.Authentication.ExchangeAlgorithmType">
          <source>The following example displays the properties of an <ph id="ph1">&lt;xref:System.Net.Security.SslStream&gt;</ph>.</source>
          <target state="translated">Nell'esempio seguente vengono visualizzate le proprietà di un <ph id="ph1">&lt;xref:System.Net.Security.SslStream&gt;</ph>.</target>       </trans-unit>
        <trans-unit id="104" translate="yes" xml:space="preserve" uid="F:System.Security.Authentication.ExchangeAlgorithmType.DiffieHellman">
          <source>The Diffie Hellman ephemeral key exchange algorithm.</source>
          <target state="translated">Algoritmo effimero di scambio delle chiavi Diffie-Hellman.</target>       </trans-unit>
        <trans-unit id="105" translate="yes" xml:space="preserve" uid="F:System.Security.Authentication.ExchangeAlgorithmType.None">
          <source>No key exchange algorithm is used.</source>
          <target state="translated">Nessun algoritmo di scambio delle chiavi utilizzato.</target>       </trans-unit>
        <trans-unit id="106" translate="yes" xml:space="preserve" uid="F:System.Security.Authentication.ExchangeAlgorithmType.RsaKeyX">
          <source>The RSA public-key exchange algorithm.</source>
          <target state="translated">Algoritmo di scambio della chiave pubblica RSA.</target>       </trans-unit>
        <trans-unit id="107" translate="yes" xml:space="preserve" uid="F:System.Security.Authentication.ExchangeAlgorithmType.RsaSign">

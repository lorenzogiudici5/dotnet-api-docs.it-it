        <trans-unit id="101" translate="yes" xml:space="preserve" uid="T:System.Data.Entity.Design.EntityStoreSchemaFilterObjectTypes">
          <source>Specifies the store object type to apply the schema filter to.</source>
          <target state="translated">Specifica il tipo di oggetto dell'archivio a cui applicare il filtro dello schema.</target>       </trans-unit>
        <trans-unit id="102" translate="yes" xml:space="preserve" extradata="MT" uid="T:System.Data.Entity.Design.EntityStoreSchemaFilterObjectTypes">
          <source>The <ph id="ph1">&lt;xref:System.Data.Entity.Design.EntityStoreSchemaFilterObjectTypes&gt;</ph> enumeration is used with the <ph id="ph2">&lt;xref:System.Data.Entity.Design.EntityStoreSchemaFilterEntry&gt;</ph> class to allow or exclude particular database objects during generation.</source>
          <target state="translated">L'enumerazione <ph id="ph1">&lt;xref:System.Data.Entity.Design.EntityStoreSchemaFilterObjectTypes&gt;</ph> viene utilizzata con la classe <ph id="ph2">&lt;xref:System.Data.Entity.Design.EntityStoreSchemaFilterEntry&gt;</ph> per consentire o escludere oggetti di database specifici durante la generazione.</target>       </trans-unit>
        <trans-unit id="103" translate="yes" xml:space="preserve" uid="F:System.Data.Entity.Design.EntityStoreSchemaFilterObjectTypes.All">
          <source>Apply the filter to all object types.</source>
          <target state="translated">Applica il filtro a tutti i tipi di oggetti.</target>       </trans-unit>
        <trans-unit id="104" translate="yes" xml:space="preserve" uid="F:System.Data.Entity.Design.EntityStoreSchemaFilterObjectTypes.Function">
          <source>Apply this filter to function object types.</source>
          <target state="translated">Applica il filtro ai tipi di oggetto funzione.</target>       </trans-unit>
        <trans-unit id="105" translate="yes" xml:space="preserve" uid="F:System.Data.Entity.Design.EntityStoreSchemaFilterObjectTypes.None">
          <source>Initial value.</source>
          <target state="translated">Valore iniziale.</target>       </trans-unit>
        <trans-unit id="106" translate="yes" xml:space="preserve" uid="F:System.Data.Entity.Design.EntityStoreSchemaFilterObjectTypes.None">
          <source>This is not a valid value to apply.</source>
          <target state="translated">Non si tratta di un valore valido da applicare.</target>       </trans-unit>
        <trans-unit id="107" translate="yes" xml:space="preserve" uid="F:System.Data.Entity.Design.EntityStoreSchemaFilterObjectTypes.Table">
          <source>Apply the filter to table object types.</source>
          <target state="translated">Applica il filtro ai tipi di oggetto tabella.</target>       </trans-unit>

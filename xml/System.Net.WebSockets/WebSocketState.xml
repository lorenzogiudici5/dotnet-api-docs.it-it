    <body>
      <group id="content" extype="content">
        <trans-unit id="101" translate="yes" xml:space="preserve" uid="T:System.Net.WebSockets.WebSocketState">
          <source>Defines the different states a WebSockets instance can be in.</source>
          <target state="translated">Definisce i diversi stati che può avere un'istanza di WebSockets.</target>       </trans-unit>
        <trans-unit id="102" translate="yes" xml:space="preserve" uid="F:System.Net.WebSockets.WebSocketState.Aborted">
          <source>Reserved for future use.</source>
          <target state="translated">Riservato per utilizzi futuri.</target>       </trans-unit>
        <trans-unit id="103" translate="yes" xml:space="preserve" uid="F:System.Net.WebSockets.WebSocketState.Closed">
          <source>Indicates the WebSocket close handshake completed gracefully.</source>
          <target state="translated">Indica che l'handshake di chiusura di WebSocket è stato completato correttamente.</target>       </trans-unit>
        <trans-unit id="104" translate="yes" xml:space="preserve" uid="F:System.Net.WebSockets.WebSocketState.CloseReceived">
          <source>A close message was received from the remote endpoint.</source>
          <target state="translated">Un messaggio di chiusura è stato ricevuto dall'endpoint remoto.</target>       </trans-unit>
        <trans-unit id="105" translate="yes" xml:space="preserve" uid="F:System.Net.WebSockets.WebSocketState.CloseSent">
          <source>A close message was sent to the remote endpoint.</source>
          <target state="translated">Un messaggio di chiusura è stato inviato all'endpoint remoto.</target>       </trans-unit>
        <trans-unit id="106" translate="yes" xml:space="preserve" uid="F:System.Net.WebSockets.WebSocketState.Connecting">
          <source>The connection is negotiating the handshake with the remote endpoint.</source>
          <target state="translated">La connessione sta negoziando l'handshake con l'endpoint remoto.</target>       </trans-unit>
        <trans-unit id="107" translate="yes" xml:space="preserve" uid="F:System.Net.WebSockets.WebSocketState.None">
          <source>Reserved for future use.</source>
          <target state="translated">Riservato per utilizzi futuri.</target>       </trans-unit>
        <trans-unit id="108" translate="yes" xml:space="preserve" uid="F:System.Net.WebSockets.WebSocketState.Open">
          <source>The initial state after the HTTP handshake has been completed.</source>
          <target state="translated">Stato iniziale dopo il completamento dell'handshake HTTP.</target>       </trans-unit>

        <trans-unit id="101" translate="yes" xml:space="preserve" uid="T:System.Data.Services.Client.SendingRequestEventArgs">
          <source>Used with the <ph id="ph1">&lt;see cref="E:System.Data.Services.Client.DataServiceContext.SendingRequest" /&gt;</ph> event to provide access to the <ph id="ph2">&lt;see cref="T:System.Net.HttpWebRequest" /&gt;</ph> instance that the client is about to send to the target data service.</source>
          <target state="translated">Utilizzato con l'evento <ph id="ph1">&lt;see cref="E:System.Data.Services.Client.DataServiceContext.SendingRequest" /&gt;</ph> per fornire accesso all'istanza di <ph id="ph2">&lt;see cref="T:System.Net.HttpWebRequest" /&gt;</ph> che il client sta per inviare al servizio dati di destinazione.</target>       </trans-unit>
        <trans-unit id="102" translate="yes" xml:space="preserve" uid="P:System.Data.Services.Client.SendingRequestEventArgs.Request">
          <source>Gets or sets the <ph id="ph1">&lt;see cref="T:System.Net.HttpWebRequest" /&gt;</ph> instance about to be sent by the client library to the data service.</source>
          <target state="translated">Ottiene o imposta l'istanza di <ph id="ph1">&lt;see cref="T:System.Net.HttpWebRequest" /&gt;</ph> che sta per essere inviata dalla libreria client al servizio dati.</target>       </trans-unit>
        <trans-unit id="103" translate="yes" xml:space="preserve" extradata="MT" uid="P:System.Data.Services.Client.SendingRequestEventArgs.Request">
          <source><ph id="ph1">&lt;see cref="T:System.Net.HttpWebRequest" /&gt;</ph>.</source>
          <target state="translated"><ph id="ph1">&lt;see cref="T:System.Net.HttpWebRequest" /&gt;</ph>.</target>       </trans-unit>
        <trans-unit id="104" translate="yes" xml:space="preserve" uid="P:System.Data.Services.Client.SendingRequestEventArgs.RequestHeaders">
          <source>Gets the collection protocol headers that are associated with the request to the data service.</source>
          <target state="translated">Ottiene le intestazioni del protocollo di raccolta associate alla richiesta al servizio dati.</target>       </trans-unit>
        <trans-unit id="105" translate="yes" xml:space="preserve" extradata="MT" uid="P:System.Data.Services.Client.SendingRequestEventArgs.RequestHeaders">
          <source>A collection of protocol headers that are associated with the request.</source>

      <group id="content" extype="content">
        <trans-unit id="101" translate="yes" xml:space="preserve" uid="T:System.Net.NetworkInformation.NetworkInformationAccess">
          <source>Specifies permission to access information about network interfaces and traffic statistics.</source>
          <target state="translated">Specifica i permessi necessari per accedere alle informazioni relative alle interfacce di rete e alle statistiche sul traffico.</target>       </trans-unit>
        <trans-unit id="102" translate="yes" xml:space="preserve" extradata="MT" uid="T:System.Net.NetworkInformation.NetworkInformationAccess">
          <source>This enumeration is used with the <ph id="ph1">&lt;xref:System.Net.NetworkInformation.NetworkInformationPermission&gt;</ph> class to specify the level of access to information that the permission object controls.</source>
          <target state="translated">Questa enumerazione viene utilizzata con la <ph id="ph1">&lt;xref:System.Net.NetworkInformation.NetworkInformationPermission&gt;</ph> classe per specificare il livello di accesso alle informazioni che controlla l'oggetto di autorizzazione.</target>       </trans-unit>
        <trans-unit id="103" translate="yes" xml:space="preserve" extradata="MT" uid="T:System.Net.NetworkInformation.NetworkInformationAccess">
          <source>The following code example creates a permission that controls Read access to network information.</source>
          <target state="translated">Esempio di codice seguente crea un'autorizzazione che controlla l'accesso in lettura alle informazioni di rete.</target>       </trans-unit>
        <trans-unit id="104" translate="yes" xml:space="preserve" uid="F:System.Net.NetworkInformation.NetworkInformationAccess.None">
          <source>No access to network information.</source>
          <target state="translated">Nessun accesso alle informazioni di rete.</target>       </trans-unit>
        <trans-unit id="105" translate="yes" xml:space="preserve" uid="F:System.Net.NetworkInformation.NetworkInformationAccess.Ping">
          <source>Ping access to network information.</source>
          <target state="translated">Accesso ping alle informazioni di rete.</target>       </trans-unit>
        <trans-unit id="106" translate="yes" xml:space="preserve" uid="F:System.Net.NetworkInformation.NetworkInformationAccess.Read">
          <source>Read access to network information.</source>
          <target state="translated">Accesso in lettura alle informazioni di rete.</target>       </trans-unit>

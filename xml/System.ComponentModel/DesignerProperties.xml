      <group id="content" extype="content">
        <trans-unit id="101" translate="yes" xml:space="preserve" uid="T:System.ComponentModel.DesignerProperties">
          <source>Provides attached properties used to communicate with a designer.</source>
          <target state="translated">Fornisce proprietà associate utilizzate per comunicare con una finestra di progettazione.</target>       </trans-unit>
        <trans-unit id="102" translate="yes" xml:space="preserve" extradata="MT" uid="T:System.ComponentModel.DesignerProperties">
          <source>The <ph id="ph1">&lt;xref:System.ComponentModel.DesignerProperties&gt;</ph> class provides attached properties that can be used to query the state of a control when it is running in a designer.</source>
          <target state="translated">La <ph id="ph1">&lt;xref:System.ComponentModel.DesignerProperties&gt;</ph> classe fornisce proprietà associate che può essere usata per eseguire query sullo stato di un controllo quando viene eseguita in una finestra di progettazione.</target>       </trans-unit>
        <trans-unit id="103" translate="yes" xml:space="preserve" extradata="MT" uid="T:System.ComponentModel.DesignerProperties">
          <source>Designer tools will set values for properties on objects that are running in the designer.</source>
          <target state="translated">Gli strumenti di progettazione verranno impostare valori per le proprietà sugli oggetti che sono in esecuzione nella finestra di progettazione.</target>       </trans-unit>
        <trans-unit id="104" translate="yes" xml:space="preserve" uid="M:System.ComponentModel.DesignerProperties.GetIsInDesignMode(System.Windows.DependencyObject)">
          <source>The element from which the property value is read.</source>
          <target state="translated">Elemento da cui viene letto il valore della proprietà.</target>       </trans-unit>
        <trans-unit id="105" translate="yes" xml:space="preserve" uid="M:System.ComponentModel.DesignerProperties.GetIsInDesignMode(System.Windows.DependencyObject)">
          <source>Gets the value of the <ph id="ph1">&lt;see cref="P:System.ComponentModel.DesignerProperties.IsInDesignMode" /&gt;</ph> attached property for the specified <ph id="ph2">&lt;see cref="T:System.Windows.UIElement" /&gt;</ph>.</source>
          <target state="translated">Ottiene il valore della proprietà associata <ph id="ph1">&lt;see cref="P:System.ComponentModel.DesignerProperties.IsInDesignMode" /&gt;</ph> per l'elemento <ph id="ph2">&lt;see cref="T:System.Windows.UIElement" /&gt;</ph> specificato.</target>       </trans-unit>
        <trans-unit id="106" translate="yes" xml:space="preserve" uid="M:System.ComponentModel.DesignerProperties.GetIsInDesignMode(System.Windows.DependencyObject)">
          <source>The <ph id="ph1">&lt;see cref="P:System.ComponentModel.DesignerProperties.IsInDesignMode" /&gt;</ph> property value for the element.</source>
          <target state="translated">Valore della proprietà <ph id="ph1">&lt;see cref="P:System.ComponentModel.DesignerProperties.IsInDesignMode" /&gt;</ph> dell'elemento.</target>       </trans-unit>
        <trans-unit id="107" translate="yes" xml:space="preserve" uid="M:System.ComponentModel.DesignerProperties.GetIsInDesignMode(System.Windows.DependencyObject)">
          <source><ph id="ph1">&lt;paramref name="element" /&gt;</ph> is <ph id="ph2">&lt;see langword="null" /&gt;</ph>.</source>
          <target state="translated"><ph id="ph1">&lt;paramref name="element" /&gt;</ph> è <ph id="ph2">&lt;see langword="null" /&gt;</ph>.</target>       </trans-unit>
        <trans-unit id="108" translate="yes" xml:space="preserve" uid="F:System.ComponentModel.DesignerProperties.IsInDesignModeProperty">
          <source>Identifies the <ph id="ph1">&lt;see cref="P:System.ComponentModel.DesignerProperties.IsInDesignMode" /&gt;</ph> attached property.</source>
          <target state="translated">Identifica la proprietà associata <ph id="ph1">&lt;see cref="P:System.ComponentModel.DesignerProperties.IsInDesignMode" /&gt;</ph>.</target>       </trans-unit>
        <trans-unit id="109" translate="yes" xml:space="preserve" extradata="MT" uid="F:System.ComponentModel.DesignerProperties.IsInDesignModeProperty">
          <source>The identifier for the <ph id="ph1">&lt;xref:System.ComponentModel.DesignerProperties.IsInDesignMode%2A&gt;</ph> attached property.</source>
          <target state="translated">Identificatore della proprietà associata <ph id="ph1">&lt;xref:System.ComponentModel.DesignerProperties.IsInDesignMode%2A&gt;</ph>.</target>       </trans-unit>
        <trans-unit id="110" translate="yes" xml:space="preserve" uid="M:System.ComponentModel.DesignerProperties.SetIsInDesignMode(System.Windows.DependencyObject,System.Boolean)">
          <source>The element to which the attached property is written.</source>
          <target state="translated">Elemento in cui viene scritta la proprietà associata.</target>       </trans-unit>
        <trans-unit id="111" translate="yes" xml:space="preserve" uid="M:System.ComponentModel.DesignerProperties.SetIsInDesignMode(System.Windows.DependencyObject,System.Boolean)">
          <source>The needed <ph id="ph1">&lt;see cref="T:System.Boolean" /&gt;</ph> value.</source>
          <target state="translated">Valore <ph id="ph1">&lt;see cref="T:System.Boolean" /&gt;</ph> necessario.</target>       </trans-unit>
        <trans-unit id="112" translate="yes" xml:space="preserve" uid="M:System.ComponentModel.DesignerProperties.SetIsInDesignMode(System.Windows.DependencyObject,System.Boolean)">
          <source>Sets the value of the <ph id="ph1">&lt;see cref="P:System.ComponentModel.DesignerProperties.IsInDesignMode" /&gt;</ph> attached property to a specified element.</source>
          <target state="translated">Imposta il valore della proprietà associata <ph id="ph1">&lt;see cref="P:System.ComponentModel.DesignerProperties.IsInDesignMode" /&gt;</ph> su un elemento specificato.</target>       </trans-unit>

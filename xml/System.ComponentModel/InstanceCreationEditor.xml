      <group id="content" extype="content">
        <trans-unit id="101" translate="yes" xml:space="preserve" uid="T:System.ComponentModel.InstanceCreationEditor">
          <source>Creates an instance of a particular type of property from a drop-down box within the <ph id="ph1">&lt;see cref="T:System.Windows.Forms.PropertyGrid" /&gt;</ph>.</source>
          <target state="translated">Crea un'istanza di un particolare tipo di proprietà da una casella a discesa nell'oggetto <ph id="ph1">&lt;see cref="T:System.Windows.Forms.PropertyGrid" /&gt;</ph>.</target>       </trans-unit>
        <trans-unit id="102" translate="yes" xml:space="preserve" extradata="MT" uid="T:System.ComponentModel.InstanceCreationEditor">
          <source>Usually, the text specified by <ph id="ph1">&lt;xref:System.ComponentModel.InstanceCreationEditor.Text%2A&gt;</ph> is displayed on the drop-down box from the <ph id="ph2">&lt;xref:System.Windows.Forms.PropertyGrid&gt;</ph> as a link or button.</source>
          <target state="translated">In genere, il testo specificato dal <ph id="ph1">&lt;xref:System.ComponentModel.InstanceCreationEditor.Text%2A&gt;</ph> viene visualizzato nella casella di riepilogo a discesa dal <ph id="ph2">&lt;xref:System.Windows.Forms.PropertyGrid&gt;</ph> come un collegamento o un pulsante.</target>       </trans-unit>
        <trans-unit id="103" translate="yes" xml:space="preserve" extradata="MT" uid="T:System.ComponentModel.InstanceCreationEditor">
          <source>When clicked, the <ph id="ph1">&lt;xref:System.ComponentModel.InstanceCreationEditor.CreateInstance%2A&gt;</ph> method is called with the <ph id="ph2">&lt;xref:System.Type&gt;</ph> of the object to create.</source>
          <target state="translated">Quando si fa clic, il <ph id="ph1">&lt;xref:System.ComponentModel.InstanceCreationEditor.CreateInstance%2A&gt;</ph> metodo viene chiamato con il <ph id="ph2">&lt;xref:System.Type&gt;</ph> dell'oggetto da creare.</target>       </trans-unit>
        <trans-unit id="104" translate="yes" xml:space="preserve" uid="M:System.ComponentModel.InstanceCreationEditor.#ctor">
          <source>Initializes a new instance of the <ph id="ph1">&lt;see cref="T:System.ComponentModel.InstanceCreationEditor" /&gt;</ph> class.</source>
          <target state="translated">Inizializza una nuova istanza della classe <ph id="ph1">&lt;see cref="T:System.ComponentModel.InstanceCreationEditor" /&gt;</ph>.</target>       </trans-unit>
        <trans-unit id="105" translate="yes" xml:space="preserve" uid="M:System.ComponentModel.InstanceCreationEditor.CreateInstance(System.ComponentModel.ITypeDescriptorContext,System.Type)">
          <source>The context information.</source>
          <target state="translated">Informazioni contestuali.</target>       </trans-unit>
        <trans-unit id="106" translate="yes" xml:space="preserve" uid="M:System.ComponentModel.InstanceCreationEditor.CreateInstance(System.ComponentModel.ITypeDescriptorContext,System.Type)">
          <source>The specified type.</source>
          <target state="translated">Tipo specificato.</target>       </trans-unit>
        <trans-unit id="107" translate="yes" xml:space="preserve" uid="M:System.ComponentModel.InstanceCreationEditor.CreateInstance(System.ComponentModel.ITypeDescriptorContext,System.Type)">
          <source>When overridden in a derived class, returns an instance of the specified type.</source>
          <target state="translated">Quando è sottoposto a override in una classe derivata, restituisce un'istanza del tipo specificato.</target>       </trans-unit>
        <trans-unit id="108" translate="yes" xml:space="preserve" uid="M:System.ComponentModel.InstanceCreationEditor.CreateInstance(System.ComponentModel.ITypeDescriptorContext,System.Type)">
          <source>An instance of the specified type or <ph id="ph1">&lt;see langword="null" /&gt;</ph>.</source>
          <target state="translated">Istanza del tipo specificato o valore <ph id="ph1">&lt;see langword="null" /&gt;</ph>.</target>       </trans-unit>
        <trans-unit id="109" translate="yes" xml:space="preserve" uid="P:System.ComponentModel.InstanceCreationEditor.Text">
          <source>Gets the specified text.</source>
          <target state="translated">Ottiene il testo specificato.</target>       </trans-unit>
        <trans-unit id="110" translate="yes" xml:space="preserve" extradata="MT" uid="P:System.ComponentModel.InstanceCreationEditor.Text">
          <source>The specified text.</source>
          <target state="translated">Il testo specificato.</target>       </trans-unit>

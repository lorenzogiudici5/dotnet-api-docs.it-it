        <trans-unit id="101" translate="yes" xml:space="preserve" uid="T:System.Windows.Forms.CaptionButton">
          <source>Specifies the type of caption button to display.</source>
          <target state="translated">Consente di specificare il tipo di pulsante della barra del titolo da visualizzare.</target>       </trans-unit>
        <trans-unit id="102" translate="yes" xml:space="preserve" extradata="MT" uid="T:System.Windows.Forms.CaptionButton">

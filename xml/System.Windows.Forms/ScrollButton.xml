        <trans-unit id="101" translate="yes" xml:space="preserve" uid="T:System.Windows.Forms.ScrollButton">
          <source>Specifies the type of scroll arrow to draw on a scroll bar.</source>
          <target state="translated">Specifica il tipo di freccia di scorrimento da disegnare su una barra di scorrimento.</target>       </trans-unit>
        <trans-unit id="102" translate="yes" xml:space="preserve" extradata="MT" uid="T:System.Windows.Forms.ScrollButton">

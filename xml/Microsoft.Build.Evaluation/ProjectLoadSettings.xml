      <group id="content" extype="content">
        <trans-unit id="101" translate="yes" xml:space="preserve" uid="T:Microsoft.Build.Evaluation.ProjectLoadSettings">
          <source>Flags for controlling the project load.</source>
          <target state="translated">Flag per il controllo del caricamento del progetto.</target>       </trans-unit>
        <trans-unit id="102" translate="yes" xml:space="preserve" extradata="MT" uid="T:Microsoft.Build.Evaluation.ProjectLoadSettings">
          <source>This is a "flags" enum, allowing future settings to be added in an additive, non breaking fashion.</source>
          <target state="translated">Questa è un'enumerazione "flags", perché consente di aggiungere in modo sostanziale additive, non impostazioni future.</target>       </trans-unit>
        <trans-unit id="103" translate="yes" xml:space="preserve" uid="F:Microsoft.Build.Evaluation.ProjectLoadSettings.Default">
          <source>Normal load.</source>
          <target state="translated">Caricamento normale.</target>       </trans-unit>
        <trans-unit id="104" translate="yes" xml:space="preserve" uid="F:Microsoft.Build.Evaluation.ProjectLoadSettings.Default">
          <source>This is the default.</source>
          <target state="translated">Questa è l'impostazione predefinita.</target>       </trans-unit>
        <trans-unit id="105" translate="yes" xml:space="preserve" uid="F:Microsoft.Build.Evaluation.ProjectLoadSettings.IgnoreMissingImports">
          <source>Ignore nonexistent targets files when evaluating the project.</source>
          <target state="translated">Ignorare i file targets inesistenti durante la valutazione del progetto.</target>       </trans-unit>
        <trans-unit id="106" translate="yes" xml:space="preserve" uid="F:Microsoft.Build.Evaluation.ProjectLoadSettings.RecordDuplicateButNotCircularImports">
          <source>Indicates that imports including duplicate, but not circular, imports on the <bpt id="p1">&lt;c&gt;</bpt>ImportsIncludingDuplicates<ept id="p1">&lt;/c&gt;</ept> property should be recorded</source>
          <target state="translated">Indica che devono essere registrate le importazioni che includono importazioni duplicate, ma non circolari, sulla proprietà <bpt id="p1">&lt;c&gt;</bpt>ImportsIncludingDuplicates<ept id="p1">&lt;/c&gt;</ept></target>       </trans-unit>
        <trans-unit id="107" translate="yes" xml:space="preserve" uid="F:Microsoft.Build.Evaluation.ProjectLoadSettings.RejectCircularImports">
          <source>Indicates that an exception will be thrown if any circular imports are detected</source>
          <target state="translated">Indica che sarà generata un'eccezione se viene rilevata un importazione circolare</target>       </trans-unit>
